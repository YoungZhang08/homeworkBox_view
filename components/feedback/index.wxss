.page {
  height: 100%;
}
.feedback {
  width: 100%;
  height: 100%;
  background-color: #eeeeee;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.feedback text {
  width: 700rpx;
  height: 32rpx;
  line-height: 32rpx;
  padding-top: 40rpx;
  font-size: 32rpx;
  text-indent: 20rpx;
}
.feedback textarea {
  width: 660rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  margin-top: 40rpx;
  height: 300rpx;
  background-color: white;
}
.feedback button {
  width: 700rpx;
  height: 60rpx;
  background-color: #73bdf6;
  color: white;
  font-size: 32rpx;
  line-height: 60rpx;
  border-radius: 0;
  margin-top: 508rpx;
}