.boxes {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  width: 100%;
  height: 100%;
  padding: 0;
}
.boxes-back {
  width: 100%;
  height: 400rpx;
}
.boxes-back image{
  width: 100%;
  height: 100%;
}
.box-content {
  width: 650rpx;
  height: 100%;
}
.box-tab {
  width: 650rpx;
  height: 120rpx;
  border-bottom: 1px solid #e4e4e4;
}
.tab-left {
  width: 306rpx;
  height: 70rpx;
  float: left;
  margin-top: 50rpx;
}
.tab-right {
  width: 50rpx;
  height: 50rpx;
  float: right;
  margin-top: 48rpx;
}
.tab-right image {
  width: 100%;
  height: 100%;
}
.popup {
  width: 450rpx;
  height: 438rpx;
}
