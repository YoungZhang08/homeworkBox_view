/* components/boxContentTab/index.wxss */
.hw-boxcontent-tab {
  width: 650rpx;
  height: 120rpx;
  border-bottom: 1px solid #e4e4e4;
}
.tab-left {
  width: 306rpx;
  height: 48rpx;
  float: left;
  display: flex;
  justify-content: space-between;
  margin-top: 50rpx;
}
.tab-left button {
  display: inline-block;
  text-align: center;
  line-height: 48rpx;
  width: 128rpx;
  height: 48rpx;
  font-size: 32rpx;
  margin: 0;
  padding: 0;
  background-color: #fff;
  border: none !important;
}
.tab-right {
  width: 50rpx;
  height: 50rpx;
  float: right;
  margin-top: 48rpx;
}
.tab-right image {
  width: 100%;
  height: 100%;
}