.boxes {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  width: 100%;
  height: 100%;
  padding: 0;
}
.boxes-back {
  width: 100%;
  height: 400rpx;
}
.boxes-back image{
  width: 100%;
  height: 100%;
}
.boxes-content {
  width: 100%;
  height: 500rpx;
  border: 1px solid red;
  /* background-color: #e4e4e4; */
}
.boxcontent-tab {
  width: 650rpx;
  height: 120rpx;
  background-color: #eeeeee;
  border-bottom: 1px solid #e4e4e4;
}
.boxes-tab {
  width: 650rpx;
  height: 120rpx;
  border-bottom: 1px solid #e4e4e4;
}
.boxes-list {
  width: 650rpx;
  height: 120rpx;
  margin-top: 22rpx;
  /* border: 1px solid black; */
}
.tab-left {
  width: 306rpx;
  height: 48rpx;
  float: left;
  display: flex;
  justify-content: space-between;
  margin-top: 50rpx;
}
.tab-left button {
  display: inline-block;
  text-align: center;
  line-height: 48rpx;
  width: 128rpx;
  height: 48rpx;
  font-size: 32rpx;
  margin: 0;
  padding: 0;
  background-color: #fff;
  border: none !important;
}
.tab-right {
  width: 50rpx;
  height: 50rpx;
  float: right;
  margin-top: 48rpx;
}
.tab-right image {
  width: 100%;
  height: 100%;
}
.popup {
  width: 450rpx;
  height: 438rpx;
}