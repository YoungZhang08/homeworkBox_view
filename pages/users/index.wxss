  justify-content: center;
  align-items: center;
  width: 100%;
  height: 500rpx;
}
.top image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
}
.top text {
  margin-top: 20rpx;
  font-size: 30rpx;
}
